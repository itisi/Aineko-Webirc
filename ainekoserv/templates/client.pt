 <div metal:use-macro="layout">
    <div tal:omit-tag="1" metal:fill-slot="add_css">
        <link href="//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css" rel="stylesheet"></link>
    </div>
    <div tal:omit-tag="1" metal:fill-slot="add_js">
        <script type="text/javascript" src="/static/jquery.1.10.2.js"></script>
        <script type="text/javascript" src="/static/socket.io.js"></script>
        <script type="text/javascript" src="/static/aineko.js"></script>
    </div>
    <div metal:fill-slot="content" tal:omit-tag="">
        <div id="hotkeyCont">
            <div class="hotkeyBlock"><span class="giant hotkey">`</span><span class="goup">Advance Channel</span></div>
        </div>
        <div id="bodycont">
            <div class="w20">
                <div>
                    <div id="channels"></div>
                    <button id="joinChannel"><span class="hotkey">J.</span>Join Channel</button>
                </div>
            </div>
            <div class="w60">
                <div id="messagecont">
                    <div id="topic"></div>
                    <div id="messages"></div>
                </div>
            </div>
            <div class="w20">
                <div id="users"></div>
            </div>
        </div>
        <div id="inputcont">
            <form id="chatform">
                <input type="text" id="chatinput">
            </form>
        </div>
    </div>
</div>
