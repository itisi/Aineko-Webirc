 <div metal:use-macro="layout">
    <div tal:omit-tag="1" metal:fill-slot="add_header">
        <script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
        <script type="text/javascript" src="/static/jquery-ui-1.8.22.custom.min.js"></script>
        <script src="/static/socket.io.js" type="text/javascript"></script>
        <script type="text/javascript" src="/static/aineko.js"></script>
    </div>
    <div metal:fill-slot="content" tal:omit-tag="">
        <div id="bodycont">
            <div class="w20">
                <div id="channels"></div>
            </div>
            <div class="w60">
                <div id="messages">
                    <div class="channel" id="servermessages"></div>
                </div>
            </div>
            <div class="w20">
                <div id="users"></div>
            </div>
        </div>
        <div id="inputcont">
            <form id="chatform">
                <input type="text" id="chatinput">
            </form>
        </div>
    </div>
</div>
